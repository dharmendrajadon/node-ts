{
  "name": "pro_cm",
  "version": "1.0.0",
  "description": "App for political campaigns.",
  "main": "build/dist/index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "node build/dist/server.js",
    "watch-node": "nodemon build/dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "engines": {
    "node": "10.8.0",
    "npm": "6.2.0"
  },
  "repository": {},
  "keywords": [
    "India",
    "Politics",
    "Telangana",
    "Andhra",
    "Pradesh"
  ],
  "author": "Dharmendra Jadon",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express-session": "^1.15.10",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/lodash": "^4.14.116",
    "@types/lusca": "^1.5.0",
    "@types/mongoose": "^5.2.17",
    "@types/passport": "^0.4.6",
    "@types/passport-jwt": "^3.0.1",
    "@types/passport-local": "^1.0.33",
    "@types/request": "^2.47.1",
    "@types/request-promise": "^4.1.42",
    "@types/shelljs": "^0.8.0",
    "@types/tmp": "0.0.33",
    "bcrypt": "^3.0.1",
    "bluebird": "^3.5.1",
